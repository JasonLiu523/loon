# 2023-09-19 20:07:19
# https://raw.githubusercontent.com/Keywos/rule/main/script/netisp/netisp.plugin


[Rule]
DOMAIN-SUFFIX, speedtest.cn, DIRECT
DOMAIN-SUFFIX, ip.im, DIRECT
DOMAIN-SUFFIX, ip.plus, DIRECT


[Script]
generic script-path = https://raw.githubusercontent.com/sooyaaabo/Loon/main/Script/NodeTools/NodeSpeedtest.js, timeout = 10, tag = 网络延迟 𝕂

generic script-path = https://raw.githubusercontent.com/sooyaaabo/Loon/main/Script/NodeTools/NodeUnlockDetection.js, timeout = 20, tag = 节点解锁查询 𝕂

generic script-path = https://raw.githubusercontent.com/sooyaaabo/Loon/main/Script/NodeTools/NodeLocation.js, timeout = 10, tag = 节点位置查询 𝕂

generic script-path = https://raw.githubusercontent.com/Keywos/rule/master/script/netisp/js/loon.js, tag = 入口落地查询 𝕂

http-request ^https?:\/\/net-lsp-x\.com script-path = https://raw.githubusercontent.com/xream/scripts/main/surge/modules/network-info/net-lsp-x.js, timeout = 120, tag = 网络信息请求 𝕏

network-changed script-path = https://raw.githubusercontent.com/xream/scripts/main/surge/modules/network-info/net-lsp-x.js, timeout = 120, tag = 网络信息变化 𝕏, argument = "TYPE=EVENT&icon=globe.asia.australia&icon-color=#6699FF&LAN=0&SSID=0&IPv6==0&MASK=0&DOMSTIC_IPv4=spcn&DOESTIC_IPv6=ddnspod&LANDING_IPv4=ipapi&LANDING_IPv6=ipsb&PRIVACY=0&FLAG=1&ENYRANCE_DELAY&EVENT_DELAY=3"

generic script-path = https://raw.githubusercontent.com/xream/scripts/main/surge/modules/network-info/net-lsp-x.js, timeout = 120, tag = 网络信息查询 𝕏

generic script-path=https://raw.githubusercontent.com/getsomecat/Loon/main/Net_Speedlite.js, timeout=10, tag=网络延迟

generic script-path=https://github.com/Keywos/rule/raw/main/loon/ipapi2.js, timeout=10, tag=地理位置查询

generic script-path=https://kelee.one/Resource/Script/Node_detection_tool/NodeUnlockDetection.js, timeout=20, tag=节点解锁查询

generic script-path=https://raw.githubusercontent.com/MCdasheng/Loon/main/Streaming/ChatGPT.js, timeout=10, tag=GPT解锁查询

generic script-path=https://raw.githubusercontent.com/MCdasheng/Loon/main/Streaming/Spotify.js, timeout=10, tag=Spotify解锁查询

generic script-path=https://raw.githubusercontent.com/Loon0x00/LoonScript/main/MediaCheck/check.js, timeout=300, tag=流媒体解锁查询

generic script-path=https://raw.githubusercontent.com/Loon0x00/LoonScript/main/MediaCheck/ytb_check.js, tag=YouTube解锁查询

generic script-path=https://raw.githubusercontent.com/Loon0x00/LoonScript/main/MediaCheck/nf_check.js, tag=Netflix解锁查询

generic script-path=https://raw.githubusercontent.com/Loon0x00/LoonScript/main/MediaCheck/disney_check.js, tag=Disney解锁查询

generic script-path=https://raw.githubusercontent.com/Loon0x00/LoonScript/main/MediaCheck/param_check.js, tag=Paramount解锁查询

generic script-path=https://raw.githubusercontent.com/Loon0x00/LoonScript/main/MediaCheck/dazn_check.js, tag=Dazn解锁查询

generic script-path=https://raw.githubusercontent.com/Loon0x00/LoonScript/main/MediaCheck/discovery_check.js, tag=Discovery解锁查询


[MitM]
hostname = net-lsp-x.com
