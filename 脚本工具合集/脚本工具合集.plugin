[Rule]
DOMAIN-SUFFIX, speedtest.cn, DIRECT
DOMAIN-SUFFIX, ip.im, DIRECT
DOMAIN-SUFFIX, ip.plus, DIRECT


[Script]
generic script-path = https://raw.githubusercontent.com/sooyaaabo/Loon/main/Script/NodeTools/NodeSpeedtest.js, timeout = 10, tag = 节点网速测试 𝕂, img-url = speedometer.system

generic script-path = https://raw.githubusercontent.com/sooyaaabo/Loon/main/Script/NodeTools/NodeUnlockDetection.js, timeout = 20, tag = 节点解锁查询 𝕂, img-url = play.circle.system

generic script-path = https://raw.githubusercontent.com/sooyaaabo/Loon/main/Script/NodeTools/NodeLocation.js, timeout = 10, tag = 节点位置查询 𝕂, img-url = location.circle.system

generic script-path = https://raw.githubusercontent.com/Keywos/rule/master/script/netisp/js/loon.js, tag = 入口落地查询 𝕂, img-url = network

http-request ^https?:\/\/net-lsp-x\.com script-path = https://raw.githubusercontent.com/xream/scripts/main/surge/modules/network-info/net-lsp-x.js, timeout = 120, tag = 网络信息请求 𝕏

network-changed script-path = https://raw.githubusercontent.com/xream/scripts/main/surge/modules/network-info/net-lsp-x.js, timeout = 120, tag = 网络信息变化 𝕏, argument = "TYPE=EVENT&icon=globe.asia.australia&icon-color=#6699FF&LAN=0&SSID=0&IPv6==0&MASK=0&DOMSTIC_IPv4=spcn&DOESTIC_IPv6=ddnspod&LANDING_IPv4=ipapi&LANDING_IPv6=ipsb&PRIVACY=0&FLAG=1&ENYRANCE_DELAY&EVENT_DELAY=3"

generic script-path = https://raw.githubusercontent.com/xream/scripts/main/surge/modules/network-info/net-lsp-x.js, timeout = 120, tag = 网络信息查询 𝕏, img-url = location.circle.system

generic script-path=https://raw.githubusercontent.com/getsomecat/Loon/main/Net_Speedlite.js, timeout=10, tag=网络延迟1, img-url=https://raw.githubusercontent.com/Orz-3/mini/master/Color/Global.png

generic script-path=https://github.com/Keywos/rule/raw/main/loon/ipapi2.js, timeout=10, tag=地理位置查询1, img-url=location.circle

generic script-path=https://kelee.one/Resource/Script/Node_detection_tool/NodeUnlockDetection.js, timeout=20, tag=节点解锁查询1, img-url=play.circle.system

generic script-path=https://raw.githubusercontent.com/MCdasheng/Loon/main/Streaming/ChatGPT.js, timeout=10, tag=GPT解锁查询1, img-url=https://raw.githubusercontent.com/fmz200/wool_scripts/main/icons/chxm1023/ChatGPT3.png

generic script-path=https://raw.githubusercontent.com/MCdasheng/Loon/main/Streaming/Spotify.js, timeout=10, tag=Spotify解锁查询1, img-url=https://raw.githubusercontent.com/fmz200/wool_scripts/main/icons/erdongchanyo/Spotify.png

generic script-path=https://raw.githubusercontent.com/Loon0x00/LoonScript/main/MediaCheck/check.js, timeout=300, tag=流媒体-解锁查询1, img-url=tv.and.mediabox

generic script-path=https://raw.githubusercontent.com/Loon0x00/LoonScript/main/MediaCheck/ytb_check.js, tag=YouTube-解锁查询1, img-url=https://raw.githubusercontent.com/Koolson/Qure/master/IconSet/Color/YouTube.png

generic script-path=https://raw.githubusercontent.com/Loon0x00/LoonScript/main/MediaCheck/nf_check.js, tag=Netflix-解锁查询1, img-url=https://raw.githubusercontent.com/Koolson/Qure/master/IconSet/Color/Netflix.png

generic script-path=https://raw.githubusercontent.com/Loon0x00/LoonScript/main/MediaCheck/disney_check.js, tag=Disney-解锁查询1, img-url=https://raw.githubusercontent.com/Koolson/Qure/master/IconSet/Color/Disney+.png

generic script-path=https://raw.githubusercontent.com/Loon0x00/LoonScript/main/MediaCheck/param_check.js, tag=Paramount-解锁查询1, img-url=https://raw.githubusercontent.com/Koolson/Qure/master/IconSet/Color/Paramount.png

generic script-path=https://raw.githubusercontent.com/Loon0x00/LoonScript/main/MediaCheck/dazn_check.js, tag=Dazn-解锁查询1, img-url=https://raw.githubusercontent.com/Koolson/Qure/master/IconSet/Color/DAZN_2.png

generic script-path=https://raw.githubusercontent.com/Loon0x00/LoonScript/main/MediaCheck/discovery_check.js, tag=Discovery-解锁查询1, img-url=https://raw.githubusercontent.com/Koolson/Qure/master/IconSet/Color/discovery+.png


